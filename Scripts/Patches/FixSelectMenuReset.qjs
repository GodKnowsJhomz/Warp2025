FixSelectMenuReset = function(_)
{
  const pat = "6A 00 6A 00 6A 00 8B 01 6A 00 6A 13 6A 00 FF 90 ?? ?? ?? ?? 33 C0 E9 ?? ?? ?? ?? 8B CF 89 BB ?? ?? ?? ?? E8 ?? ?? ?? ?? 85 C0 75 0B";
  const pos = Exe.FindHex(pat);

  if (pos < 0)
    throw Error("FixSelectMenuReset pattern not found");

  Exe.SetHex(pos + pat.split(" ").length - 2, "EB 0B");

  return true;
};

FixSelectMenuReset .validate = () => Exe.BuildDate >= 20230800;
